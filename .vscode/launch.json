{
    "configurations": [
        {
            "type": "java",
            "name": "Spring Boot-IotRobotApplication<workspace>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "human.college.it.iotrobot.IotRobotApplication",
            "projectName": "workspace",
            "args": " --spring.profiles.active=dev ",
            "envFile": "${workspaceFolder}/.env",
            "vmArgs": " -Dspring.devtools.restart.enabled=true"
        }
    ]
}
